<%- layout ('layout') %>


<div class="container">

  <h1>All Created Categories</h1>
  <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
  	<thead>
  <tr>
    <td class="warning">Category Name</td>
    <td class="warning">Edit Category</td>
  </tr>
  </thead>
<% for (var i = 0; i < data.length; i++) { %>
    <tbody>
      <tr>
        <td class="active"><strong><%= data[i].name %></strong></td>
        <td class="active"><p><a class="btn btn-info" href="#" role="button">Edit &raquo;</a></p></td>
      </tr>
    </tbody>
    <% } %>

  				</table>

  <a class="newuserlink" href="/user-registration">Create New Category</a>










</div>
