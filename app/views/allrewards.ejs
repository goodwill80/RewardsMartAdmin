<%- layout('layout') -%>

<div class="container">
<div class="row">
  <% for(var i = 0; i < data.length; i++) { %>
       <div class="col-xs-18 col-sm-7 col-md-4  ">
         <div class="thumbnail">

           <img id="thumbimage" src="<%= data[i].image %>">
             <div class="caption">
               <h4><%= data[i].name %></h4>
               <p>SGD $<%= data[i].price %></p>
               <p>Points: <%= data[i].points %></p>
               <a href="#" class="btn btn-default btn-xs pull-right" role="button"><i class="glyphicon glyphicon-edit"></i></a> <a href="/rewards-admin/<%= data[i]._id %>" class="btn btn-info btn-xs" role="button">Edit/Delete</a>
            </div>

           </div>

         </div>
    <% } %>
       </div>

</div>
